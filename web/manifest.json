{
    "name": "Availabell",
    "description": "Find available candidates for work in your area",
    "version": "1.0",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "browser_action": {
        "default_icon": "/icons/Icon-192.png"
    },
    "permissions": [
        "geolocation",
        "tabs",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"]
    }
}